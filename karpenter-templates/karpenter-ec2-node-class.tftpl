---
apiVersion: karpenter.k8s.aws/v1beta1
kind: EC2NodeClass
metadata:
  name: karpenter
spec:
  amiFamily: ${karpenter_ami_family_class}
  role: ${karpenter_node_role_name}
  subnetSelectorTerms:
    - tags:
        karpenter.sh/discovery: ${eks_cluster_name}
  securityGroupSelectorTerms:
    - tags:
        karpenter.sh/discovery: ${eks_cluster_name}
  tags:
    karpenter.sh/discovery: ${eks_cluster_name}